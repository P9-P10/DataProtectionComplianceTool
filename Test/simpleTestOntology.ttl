@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix sh:    <http://www.w3.org/ns/shacl#> .
@prefix ddl:   <http://www.cs-22-dt-9-03.org/datastore-description-language#> .
@prefix sto:   <http://www.example.org/test#> .
@base          <http://www.example.org/test#> .

sto:
    a            owl:Ontology ;
    rdfs:label   "Simple Test Ontology"@en ;
    rdfs:comment "This is a comment about the test ontology"@en ;
    sh:declare   [ sh:prefix    "sto" ;
                   sh:namespace "http://www.example.org/test#" ; ] ; .


sto:T rdf:type rdfs:Class .